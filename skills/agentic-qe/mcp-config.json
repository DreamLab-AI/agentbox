{
  "name": "agentic-qe",
  "version": "2.8.2",
  "description": "Agentic QE Fleet - AI-powered quality engineering with 20 agents, 46 skills, 100 MCP tools",
  "command": "python",
  "args": ["server.py"],
  "env": {
    "AQE_COVERAGE_THRESHOLD": "95",
    "AQE_ENABLE_LEARNING": "true",
    "AQE_PARALLEL_WORKERS": "10",
    "AQE_MODEL_ROUTER": "auto",
    "AQE_RUVECTOR_ENABLED": "true",
    "AQE_PROVIDERS_CONFIG": "./config/providers.yaml",
    "AGENTDB_PATH": "./.agentic-qe/memory.db"
  },
  "providers": {
    "zai": {
      "baseUrl": "http://localhost:9600",
      "apiKeyEnv": "ZAI_ANTHROPIC_API_KEY"
    },
    "openai": {
      "baseUrl": "https://api.openai.com/v1",
      "apiKeyEnv": "OPENAI_API_KEY"
    },
    "deepseek": {
      "baseUrl": "https://api.deepseek.com/v1",
      "apiKeyEnv": "DEEPSEEK_API_KEY"
    },
    "gemini": {
      "baseUrl": "https://generativelanguage.googleapis.com/v1beta",
      "apiKeyEnv": "GOOGLE_GEMINI_API_KEY"
    }
  },
  "dependencies": {
    "npm": ["agentic-qe@^2.8.2", "agentdb@^2.0.0"],
    "python": ["mcp>=1.0.0"]
  }
}
