{
  "multiModelRouter": {
    "enabled": true,
    "version": "2.8.2",
    "defaultModel": "deepseek-chat",
    "enableCostTracking": true,
    "enableFallback": true,
    "maxRetries": 3,
    "costThreshold": 0.5,
    "modelRules": {
      "simple": {
        "model": "glm-4.7",
        "maxTokens": 2000,
        "estimatedCost": 0.001
      },
      "moderate": {
        "model": "deepseek-chat",
        "maxTokens": 4000,
        "estimatedCost": 0.002
      },
      "complex": {
        "model": "gpt-4o",
        "maxTokens": 8000,
        "estimatedCost": 0.025
      },
      "critical": {
        "model": "gpt-4o",
        "maxTokens": 16000,
        "estimatedCost": 0.05
      }
    },
    "fallbackChains": {
      "gpt-4o": ["deepseek-chat", "gemini-2.0-flash", "glm-4.7"],
      "deepseek-chat": ["glm-4.7", "gemini-2.0-flash", "gpt-4o-mini"],
      "gemini-2.0-flash": ["deepseek-chat", "glm-4.7", "gpt-4o-mini"],
      "glm-4.7": ["deepseek-chat", "gemini-2.0-flash"],
      "deepseek-reasoner": ["gpt-4o", "gemini-1.5-pro"],
      "gpt-4o-mini": ["deepseek-chat", "glm-4.7"]
    },
    "customProviders": {
      "zai": {
        "enabled": true,
        "type": "openai-compatible",
        "baseUrl": "http://localhost:9600",
        "apiKeyEnv": "ZAI_ANTHROPIC_API_KEY",
        "defaultModel": "glm-4.7",
        "models": ["glm-4.7"]
      },
      "deepseek": {
        "enabled": true,
        "type": "openai-compatible",
        "baseUrl": "https://api.deepseek.com/v1",
        "apiKeyEnv": "DEEPSEEK_API_KEY",
        "defaultModel": "deepseek-chat",
        "models": ["deepseek-chat", "deepseek-reasoner"]
      },
      "gemini": {
        "enabled": true,
        "type": "google",
        "baseUrl": "https://generativelanguage.googleapis.com/v1beta",
        "apiKeyEnv": "GOOGLE_GEMINI_API_KEY",
        "defaultModel": "gemini-2.0-flash",
        "models": ["gemini-2.0-flash", "gemini-1.5-pro"]
      },
      "openai": {
        "enabled": true,
        "type": "openai",
        "baseUrl": "https://api.openai.com/v1",
        "apiKeyEnv": "OPENAI_API_KEY",
        "defaultModel": "gpt-4o-mini",
        "models": ["gpt-4o", "gpt-4o-mini", "gpt-3.5-turbo"]
      }
    }
  },
  "streaming": {
    "enabled": true,
    "progressInterval": 2000,
    "bufferEvents": false,
    "timeout": 1800000
  }
}
